{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/login",
      "dest": "/login.html"
    },
    {
      "src": "/simulation",
      "dest": "/simulation.html"
    },
    {
      "src": "/history",
      "dest": "/history.html"
    },
    {
      "src": "/progress",
      "dest": "/progress.html"
    },
    {
      "src": "/trainer",
      "dest": "/trainer.html"
    },
    {
      "src": "/attempt-details",
      "dest": "/attempt-details.html"
    },
    {
      "src": "/test-api",
      "dest": "/test-api.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}